{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://deploy-social-media-ap1.onrender.com/api/$1"
      }
    ],
    "env": {
      "REACT_APP_API_URL": "https://deploy-social-media-ap1.onrender.com/api"
    }
  }
  